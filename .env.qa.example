# .env.qa - QA Environment Configuration
DOMAIN=qa.idhub.ibdgc.org
API_DOMAIN=api-qa.idhub.ibdgc.org

# NocoDB QA
NOCODB_DB_PASSWORD_QA=<secure-password>
JWT_SECRET_QA=<secure-jwt-secret>

# IDHub Database QA
IDHUB_DB_PASSWORD_QA=<secure-password>

# GSID Service QA
GSID_API_KEY_QA=<secure-api-key>

# AWS (shared or separate buckets)
AWS_ACCESS_KEY_ID=<aws-key>
AWS_SECRET_ACCESS_KEY=<aws-secret>
AWS_DEFAULT_REGION=us-east-1
S3_BUCKET=idhub-curated-fragments-qa

# REDCap QA
REDCAP_API_URL=https://redcap.edu/api/
REDCAP_API_TOKEN_GAP_QA=<qa-token>
REDCAP_API_TOKEN_CD_ILEAL_QA=<qa-token>
REDCAP_API_TOKEN_UC_DEMARC_QA=<qa-token>
